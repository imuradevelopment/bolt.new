PORT=4000
# FE の実 URL を指定（デフォルト: http://localhost:3000）
CORS_ORIGIN=http://localhost:3000

# LLM 設定（Gemini）
LLM_PROVIDER=gemini
GEMINI_API_KEY=
GEMINI_MODEL=gemini-2.5-pro

# 任意の調整値
LLM_MAX_TOKENS=2048
LLM_MAX_RESPONSE_SEGMENTS=3
LLM_TEMPERATURE=0.2

# SQLite の保存先（未設定時は app/BE/db/data.sqlite3）
# SQLITE_PATH=
